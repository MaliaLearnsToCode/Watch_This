<script>
    var previousOpenedSectionId = "";

    $(document).ready(function () {
        $("body:not(.first_level_element)").click(function (e) {
            $("#hiddenfooter_state").prop("checked", true);
            $(".radio_footer").prop("checked", false);

            if (e.target.type == "checkbox" && e.target.name == "footer_state") {
                if (previousOpenedSectionId == e.target.id) {
                    $("#" + e.target.id).prop("checked", false);
                    previousOpenedSectionId = "";
                }
                else {
                    $("#" + e.target.id).prop("checked", true);
                    previousOpenedSectionId = e.target.id;
                }
            }
        });
    });
</script>

    
