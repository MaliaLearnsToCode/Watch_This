<script>
    var previousFocus = "";

    $(document).ready(function () {
        function resetNav(evt) {
            if ($(window).width() < 1000)
                return;

            /* prevent reset when clicking on the nav*/
            if ($(evt.target).hasClass("nav_label") || $(evt.target).hasClass("nav_element") || $(evt.target).hasClass("radio_nav")) {
                return;
            }
            else {
                console.log("there");

                if (evt.target.id == "main_navigation_content" || evt.target.id == "main_navigation_content_elements")
                    return;
                if ($(evt.target).closest('.nav_label').length)
                    return;
                if ($(evt.target).closest('.full_nav, #main_navigation_content > *').length)
                    return;
                if ($(evt.target).closest('#main_navigation_content_elements > *').length)
                    return;
                // Do processing of click event here for every element except with id menu_content
                $('input.radio_nav').prop('checked', false);
            }
        }

        /* reset nav when clicking outside of nav element */
        $('body').click(function (evt) {
            resetNav(evt);
        });

        /* touchstart event for ipad nav reset */
        $('body').on('touchstart', function (evt) {
            resetNav(evt);
        });

        /* reset nav when clicking on another element of the nav */
        $('input.radio_nav').on('change', function () {
            //if ($(window).width() < 1000)
            //    return;
            $('input.radio_nav').not(this).prop('checked', false);
        });
        $(window).bind('beforeunload', function (evt) {
            resetNav(evt);
            $('input#navigation_state').prop('checked', false);
            $('input#collection_state_mobile').prop('checked', false);
        });
        var lastWindowWidth = $(window).width();
        $(window).resize(function (evt) {
            if ($(window).width() > 1000 && lastWindowWidth <= 1000) {
                resetNav(evt);
                $('input#navigation_state').prop('checked', false);
                $('input#collection_state_mobile').prop('checked', false);
            }
            lastWindowWidth = $(window).width();
        });
        $(".nav_element").click(function () {
            if (previousFocus == $(this)[0].className) {
                $(this).blur();
                previousFocus = "";
            }
            else {
                previousFocus = $(this)[0].className;
                console.log($(this)[0].className);
            }
        });

        /* wishlist articles:*/

        /* we get the number of articles in wishlist and update the nav counter*/
        var wishlistArticlesCount = Patek.Wishlist.getNumberOfArticles();
        $("#wishlist_counter").text(wishlistArticlesCount);

        /* depending on the presence of articles, we add or remove the has_elements*/
        /* class (displaying the counter and heart in orange)*/
        if (wishlistArticlesCount > 0) {
            $(".nav_element.wishlist_element").addClass("has_elements");
        }
        else {
            $(".nav_element.wishlist_element").removeClass("has_elements");
        }

        // to prevent blur on parent element (.nav_element)
        $("li.child_element").on('mousedown', function (e) {
            e.stopImmediatePropagation();
            e.preventDefault();
            return false;
        });
    });

    // detect scroll top from window
    $(window).scroll(function () {
        var scroll = $(window).scrollTop();

        if (scroll > 0) {
            $("html").addClass("scrolled");
        }
        else {
            $("html").removeClass("scrolled");
        }
    });
</script>

